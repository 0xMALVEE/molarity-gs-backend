  "license": "MIT",
  "dependencies": {
    "base58-encode": "^0.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "bs58": "^5.0.0",
    "concurrently": "^3.6.0",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-and-time": "^0.12.0",
    "discord.js": "^12.5.3",
    "express": "^4.16.3",
    "express-fileupload": "^1.1.6",
    "free-translate": "^0.6.0",
    "google-translate-free": "^2.4.5",
    "ipinfo": "^1.5.1",
    "jsonwebtoken": "^8.5.0",
    "live-server": "^1.2.1",
    "mongoose": "^5.1.6",
    "multer": "^1.4.2",
    "next": "^9.3.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "request-ip": "^2.1.3",
    "translate": "^1.4.1",
    "uuid": "^3.4.0",
    "uuidv4": "^6.2.10"
